		"@nextcloud/eslint-config": "^8.2.1",
		"@nextcloud/stylelint-config": "^2.3.0",
		"@nextcloud/webpack-vue-config": "^5.4.0",
		"@vue/cli-plugin-unit-jest": "^5.0.8",
		"@vue/test-utils": "^1.3.0",
		"@vue/vue2-jest": "^29.0.0",
		"vue-jest": "^3.0.7"
	},
	"engines": {
		"node": ">=16.0.0",
		"npm": "^7.0.0 || ^8.0.0"
	}
